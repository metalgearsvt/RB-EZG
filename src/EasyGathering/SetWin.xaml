<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="EasyGathering.SetWin"
        mc:Ignorable="d" 
        Title="EasyGathering" Height="400" Width="500" Background="#FF312F31" WindowStyle="None" AllowsTransparency="True" MouseDown="Window_MouseDown">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="img/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="UIColor" Color="#FFA1A660"/>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Button Content="{StaticResource CloseButton}" Click="Button_Click" Background="{x:Null}" BorderBrush="{x:Null}" Margin="465,10,10,365" Padding="0" Width="25" Height="25" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{x:Null}"/>
    </Grid>
</Window>
